CREATE TABLE Carro (
    chassi INT(11) NOT NULL,
    nome VARCHAR(50) NOT NULL,
    marca VARCHAR(50) NOT NULL,
    ano INT(11) NOT NULL,
    km INT(20) NOT NULL,
    PRIMARY KEY (chassi) 
);
CREATE TABLE marca (
    id INT AUTO_INCREMENT NOT NULL,
    nome VARCHAR(100) NOT NULL,
    ano_criacao INT NOT NULL,
    dono VARCHAR(100) NOT NULL,
    PRIMARY KEY (id)
);

